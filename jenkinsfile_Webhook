pipeline {
   agent any
    stages {
         stage('pre -build') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
            }
        }
        stage('build') {
            steps {
                
                echo 'build in progress'
                sh 'python3  Webhook/main.py'
            }
        }
        stage('test') {
            steps {
                sh 'echo test-build'
                sh 'python3 --version'
            }
        }
        stage('correct') {
            steps {
                
                echo 'week6 dev tool'
                sh 'python3  Webhook/main.py'
            }
        }
        stage('finish') {
            steps {
                
                echo 'Finish'
                sh 'python3  Webhook/main.py'
            }
        }
    }
}